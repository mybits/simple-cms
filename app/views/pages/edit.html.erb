<%= link_to '<< Back to Pages List', {action: :index}, class: 'back-link' %>

<div class="page update">
	<h2>Update Page</h2>

	<%= form_for(@page, :url => {action: :update}) do |f| %>

		<%= render(partial: 'form', locals: {:f => f}) %>

		<div class="form-buttons">
			<%= f.submit("Update Page") %>
		</div>
	<% end %>

</div>